{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(json|js|jsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-format lint build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsah/starter-kit.git"
  },
  "keywords": [
    "Webpack",
    "Babel",
    "Eslint",
    "React",
    "Jest",
    "Prettier",
    "Npm",
    "Config"
  ],
  "author": "Thomas Salah",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
