{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(json|js)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list",
    "validate": "npm run check-format && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsah/starter-kit.git"
  },
  "keywords": [
    "Webpack",
    "Babel",
    "Eslint",
    "React",
    "Jest",
    "Prettier",
    "Npm",
    "Config"
  ],
  "author": "Thomas Salah",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "prettier": "^2.0.4"
  }
}
