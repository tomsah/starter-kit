{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production",
    "check-format": "npm run prettier -- --list-different",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot --open",
    "dev:debug": "npm run debug -- --env.mode development",
    "format": "npm run prettier -- --write",
    "lint": "eslint --ignore-path .gitignore .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(json|js|jsx|css|html|md)\"",
    "prod": "npm run webpack -- --env.mode production",
    "prod:analyze": "npm run prod -- --env.presets analyze",
    "prod:compress": "npm run prod -- --env.presets compress",
    "prod:allPresets": "npm run prod:compress -- --env.presets analyze",
    "prod:debug": "npm run debug -- --env.mode production",
    "start": "serve --no-clipboard --single --listen 8080 dist",
    "validate": "npm-run-all --parallel check-format lint build",
    "webpack": "webpack",
    "webpack:json-profile": "webpack --profile --json > compilation-stats.json",
    "webpack-dev-server": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsah/starter-kit.git"
  },
  "keywords": [
    "Webpack",
    "Babel",
    "Eslint",
    "React",
    "Jest",
    "Prettier",
    "Npm",
    "Config"
  ],
  "author": "Thomas Salah",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.2",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-kentcdodds": "^14.8.1",
    "eslint-config-prettier": "^6.10.1",
    "file-loader": "^6.0.0",
    "gsap": "^3.2.6",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "style-loader": "^1.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
