{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run webpack -- --env.mode production",
    "build:allPresets": "npm run build:compress -- --env.presets analyze",
    "build:analyze": "npm run build -- --env.presets analyze",
    "build:compress": "npm run build -- --env.presets compress",
    "build:debug": "npm run debug -- --env.mode production",
    "check-format": "npm run prettier -- --list-different",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "dev": "npm run webpack-dev-server -- --env.mode development --hot --open",
    "dev:debug": "npm run debug -- --env.mode development",
    "format": "npm run prettier -- --write",
    "lint": "jest --config test/jest.lint.js",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(json|js|jsx|css|html|md)\"",
    "start": "serve --no-clipboard --single --listen 8080 dist",
    "setup": "npm install && npm run validate",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "validate": "npm-run-all --parallel check-format test build",
    "webpack": "webpack",
    "webpack:json-profile": "webpack --profile --json > compilation-stats.json",
    "webpack-dev-server": "webpack-dev-server"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|css|html|md)": [
      "prettier",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsah/starter-kit.git"
  },
  "keywords": [
    "Webpack",
    "Babel",
    "Eslint",
    "React",
    "Jest",
    "Prettier",
    "Npm",
    "Config"
  ],
  "author": "Thomas Salah",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.2",
    "@testing-library/dom": "^7.5.1",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "@testing-library/user-event": "^10.1.1",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-kentcdodds": "^14.8.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-jest": "^3.0.0",
    "file-loader": "^6.0.0",
    "gsap": "^3.2.6",
    "history": "^4.10.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "http-server": "^0.12.1",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.5.4",
    "jest-axe": "^3.4.0",
    "jest-runner-eslint": "^0.7.7",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.4.1",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "style-loader": "^1.2.0",
    "test-data-bot": "^0.8.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^5.1.3"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5"
  }
}
